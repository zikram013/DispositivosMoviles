<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:ndroid="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".FinDeJuego"
    tools:ignore="Deprecated">

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/buenaPuntuacion"
        android:layout_width="358dp"
        android:layout_height="215dp"
        android:layout_x="16dp"
        android:layout_y="228dp"
        android:src="@drawable/congrats" />

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/malaPuntuacion"
        android:layout_width="358dp"
        android:layout_height="215dp"
        android:layout_x="16dp"
        android:layout_y="228dp"
        android:src="@drawable/malo" />

    <Button
        android:id="@+id/volver"
        android:layout_width="315dp"
        android:layout_height="wrap_content"
        android:layout_x="37dp"
        android:layout_y="502dp"
        android:text="@string/volver"/>

    <TextView
        android:id="@+id/puntuacionFinal"
        android:layout_width="318dp"
        android:layout_height="wrap_content"
        android:layout_x="29dp"
        android:layout_y="172dp"
        android:text="" />
</AbsoluteLayout>